<?xml version="1.0"?>
<extension key="websms" type="module">
  <file>websms</file>
  <name>websms</name>
  <description>Send and receive text messages in CiviCRM using websms.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/websms</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/websms</url>
    <url desc="Support">https://github.com/greenpeace-cee/websms/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-04-05</releaseDate>
  <version>1.1.0-beta.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.24</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Websms</namespace>
    <format>22.10.0</format>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
